<div class="container">
  <div class="page-header">
    <h1>Dewey Team</h1>
  </div>

  <div class="row">
    <% @members.each do |user| %>
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <img src="<%= user['img_url'] %>" style="width:200px; height:200px;">
          <div class="caption">
            <h3><%= user['name'] %></h3>
            <p><%= user['bio'] %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>

</div>
